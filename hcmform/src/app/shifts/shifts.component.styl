.container
    margin-left 240px
    background #f1f1f1
.row
    font-family 'Roboto', sans-serif
    .card
        background white
        border none
        border-radius 20px
        box-shadow 1px 2px 3px grey
        position relative
        .corner
            position absolute
            top -6px
            left -10px
            // border 1px solid black
            border-radius 100%
            padding 5px 10px 5px 10px
            background white
            color #458af3
            box-shadow 1px 2px 3px grey
        .card-header
            display flex
            justify-content space-between
            border none
            background white
            border-radius 20px
            font-size 13px
            padding 28px
        .card-b
            display flex
            justify-content space-between
            .card-set
                text-align center
                .para
                    font-size 12px
.pagination
    .list-menu
        margin 0px
        padding 0px
        background teal
        border-radius 10px
        display flex
        justify-content left
        align-items left
        li
            list-style-type none
            padding 16px
        .list-item:hover .list-link
            background black
            color white
            border-radius 50%
            padding 5px 8px 5px 8px
